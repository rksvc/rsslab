    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-react": "^1.4.5",
    "@types/node": "^24.10.13",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "typescript": "^5.9.3"
  }
